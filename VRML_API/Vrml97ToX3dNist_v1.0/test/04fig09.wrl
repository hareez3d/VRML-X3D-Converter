Group {
    children [
        Shape {
            appearance DEF White Appearance {
                material Material { }
            }
            geometry Text {
                string "First"
                fontStyle FontStyle {
                    family  "SERIF"
                    style   "ITALIC"
                    justify "END"
                    size    1.0
                }
            }
        },
        Shape {
            appearance USE White
            geometry Text {
                string "Second"
                fontStyle FontStyle {
                    family  "SANS"
                    style   "BOLD"
                    justify "BEGIN"
                    size    1.0
                }
            }
        }
    ]
}



